web: java
    -Dserver.port=$PORT
    -Dspring.profiles.active=$SPRING_PROFILES_ACTIVE
    -Dspring.datasource.url=$H2_URL
    -Dspring.datasource.username=$H2_US
    -Dspring.datasource.password=$H2_PW
    -Djwt.secret=$JWT_SECRET
    -Dapp.system.password=$STK_SYSTEM_PW
    -Dapp.system.username=$STK_SYSTEM_US
    -jar medpay-api/target/*.jar